{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "description": "A boilerplate of React",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "start:backend": "node test/backend.js",
    "start:dev": "babel-node server/dev.js -b env,react,stage-2",
    "start:prod": "nodemon server/index.js --exec babel-node -b env,react,stage-2",
    "build:client": "webpack --config build/webpack.client.prod.js",
    "build:server": "webpack --config build/webpack.server.prod.js"
  },
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-persist": "^5.9.1",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "browser-sync": "^2.24.5",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.4",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-node-externals": "^1.7.2"
  }
}
